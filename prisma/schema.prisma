// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model TestGPA {
  id        String   @id @default(uuid())
  author    String
  gpa       Float
  credit    Int
  actual    String
  expected  String
  result    String
  createdAt DateTime @default(now())
}
